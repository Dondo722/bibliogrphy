	person_Shogo_Kinugasa
    <- person;
    <- male;
    <- concept_writer;

    => nrel_main_idtf:
        [Сёго Кинугаса] (* <- lang_ru;; *);
		[Shogo Kinugasa] (* <- lang_en;; *);
            
    => nrel_place_of_birth:
        country_Japan;

    => nrel_languages_of_work:...
        (*
            -> lang_jp;;
        *);

    <= nrel_genres: ...
        (*
            -> concept_psychological;;
			-> concept_thriller;;
			-> concept_ranobe;;
        *);;
*];;
